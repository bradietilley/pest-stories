{
    "name": "bradietilley/pest-storyboard",
    "description": "A tool to assist in creating complex permutations of test case scenarios in Pest PHP (for Laravel)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BradieTilley\\StoryBoard\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0",
        "symfony/var-dumper": "^6.1.5",
        "laravel/pint": "^1.4",
        "phpstan/phpstan": "^1.8"
    },
    "require": {
        "illuminate/support": "^10.0",
        "illuminate/container": "^10.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BradieTilley\\StoryBoard\\StoryBoardServiceProvider"
            ]
        }
    }
}
