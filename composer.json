{
    "name": "bradietilley/pest-storyboard",
    "description": "A tool for creating complex permutations of test case scenarios in Pest PHP (for Laravel)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BradieTilley\\StoryBoard\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "^1.22",
        "symfony/var-dumper": "^6.1.5"
    },
    "require": {
        "illuminate/support": "^8.0|^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
