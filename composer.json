{
    "name": "bradietilley/pest-storyboard",
    "description": "A tool to assist in creating complex permutations of test case scenarios in Pest PHP (for Laravel)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BradieTilley\\StoryBoard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Bradie Tilley",
            "homepage": "https://github.com/bradietilley"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "pestphp/pest": "^1.22",
        "orchestra/testbench": "^7.0",
        "symfony/var-dumper": "^6.1.5",
        "laravel/pint": "dev-main",
        "phpstan/phpstan": "1.10.x-dev"
    },
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "illuminate/container": "^9.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BradieTilley\\StoryBoard\\StoryBoardServiceProvider"
            ]
        }
    }
}
